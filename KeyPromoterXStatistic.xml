<application>
  <component name="KeyPromoterXStatistic">
    <option name="statistics">
      <Statistic Action="Auto-Indent Lines">
        <Item>
          <IdeaActionID>AutoIndentLines</IdeaActionID>
          <ShortCut>'⌃⌥I'</ShortCut>
          <Description>Auto-Indent Lines</Description>
          <Count>1</Count>
        </Item>
      </Statistic>
      <Statistic Action="Declaration">
        <Item>
          <IdeaActionID>GotoDeclaration</IdeaActionID>
          <ShortCut>'⌘B' or '⌘Button1 Click' or 'Button2 Click'</ShortCut>
          <Description>Declaration</Description>
          <Count>2</Count>
        </Item>
      </Statistic>
      <Statistic Action="Hide All Windows">
        <Item>
          <IdeaActionID>HideAllWindows</IdeaActionID>
          <ShortCut>'⇧⌘F12'</ShortCut>
          <Description>Hide All Windows</Description>
          <Count>1</Count>
        </Item>
      </Statistic>
      <Statistic Action="Hide Grid">
        <Item>
          <IdeaActionID>Images.Editor.ToggleGrid</IdeaActionID>
          <ShortCut>'⌘''</ShortCut>
          <Description>Hide Grid</Description>
          <Count>1</Count>
        </Item>
      </Statistic>
      <Statistic Action="New Scratch File">
        <Item>
          <IdeaActionID>NewScratchFile</IdeaActionID>
          <ShortCut>'⇧⌘N'</ShortCut>
          <Description>New Scratch File</Description>
          <Count>1</Count>
        </Item>
      </Statistic>
      <Statistic Action="New...">
        <Item>
          <IdeaActionID>NewElement</IdeaActionID>
          <ShortCut>'⌘N' or '⌃⏎'</ShortCut>
          <Description>New...</Description>
          <Count>2</Count>
        </Item>
      </Statistic>
      <Statistic Action="Rerun 'collect_tournament_data'">
        <Item>
          <IdeaActionID>Rerun</IdeaActionID>
          <ShortCut>'⌘R'</ShortCut>
          <Description>Rerun 'collect_tournament_data'</Description>
          <Count>1</Count>
        </Item>
      </Statistic>
      <Statistic Action="Run 'collect_tournament_data'">
        <Item>
          <IdeaActionID>Run</IdeaActionID>
          <ShortCut>'⌃⌥E'</ShortCut>
          <Description>Run 'collect_tournament_data'</Description>
          <Count>2</Count>
        </Item>
      </Statistic>
      <Statistic Action="Run 'test'">
        <Item>
          <IdeaActionID>RunClass</IdeaActionID>
          <ShortCut>'⌃⇧R'</ShortCut>
          <Description>Run 'test'</Description>
          <Count>1</Count>
        </Item>
      </Statistic>
      <Statistic Action="Run 'workflowdag'">
        <Item>
          <IdeaActionID>Run</IdeaActionID>
          <ShortCut>'⌃⌥E'</ShortCut>
          <Description>Run 'workflowdag'</Description>
          <Count>2</Count>
        </Item>
      </Statistic>
      <Statistic Action="Run 'workflowdagr'">
        <Item>
          <IdeaActionID>Run</IdeaActionID>
          <ShortCut>'⌃⌥E'</ShortCut>
          <Description>Run 'workflowdagr'</Description>
          <Count>1</Count>
        </Item>
      </Statistic>
      <Statistic Action="Scientific Mode">
        <Item>
          <IdeaActionID>PySciView</IdeaActionID>
          <ShortCut>'⌃⌘S'</ShortCut>
          <Description>Scientific Mode</Description>
          <Count>4</Count>
        </Item>
      </Statistic>
      <Statistic Action="Show Grid">
        <Item>
          <IdeaActionID>Images.Editor.ToggleGrid</IdeaActionID>
          <ShortCut>'⌘''</ShortCut>
          <Description>Show Grid</Description>
          <Count>1</Count>
        </Item>
      </Statistic>
      <Statistic Action="Toggle Line Breakpoint">
        <Item>
          <IdeaActionID>ToggleLineBreakpoint</IdeaActionID>
          <ShortCut>'⌘F8'</ShortCut>
          <Description>Toggle Line Breakpoint</Description>
          <Count>1</Count>
        </Item>
      </Statistic>
      <Statistic Action="Zoom Out">
        <Item>
          <IdeaActionID>Images.Editor.ZoomOut</IdeaActionID>
          <ShortCut>'⌘-' or '⌘-'</ShortCut>
          <Description>Zoom Out</Description>
          <Count>2</Count>
        </Item>
      </Statistic>
    </option>
  </component>
</application>